{
  "name": "arrow-cluster-layer",
  "version": "0.3.1",
  "description": "deck.gl CompositeLayer for clustered point rendering from Apache Arrow tables",
  "license": "ISC",
  "keywords": [
    "deck.gl",
    "deckgl",
    "clustering",
    "layer",
    "apache-arrow",
    "geoarrow",
    "geoparquet",
    "geospatial",
    "supercluster",
    "map",
    "visualization",
    "webgl"
  ],
  "homepage": "https://arrow-cluster-layer-demo.jonathanstombaugh.workers.dev",
  "repository": {
    "type": "git",
