{
  "name": "arrow-cluster-layer-example",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "npx tsx scripts/generate-geoparquet.ts && vite build",
    "build:vite": "vite build",
    "generate-data": "npx tsx scripts/generate-geoparquet.ts",
    "upload:r2": "npx wrangler r2 object put arrow-cluster-demo-data/points-2m.parquet --file dist/data/points-2m.parquet --content-type application/vnd.apache.parquet --remote",
    "deploy": "npx wrangler deploy",
    "deploy:full": "pnpm run build && pnpm run upload:r2 && pnpm run deploy",
    "preview": "npx wrangler dev"
  },
  "dependencies": {
    "@deck.gl/core": "^9.1.10",
    "@deck.gl/geo-layers": "^9.1.10",
    "@deck.gl/layers": "^9.1.10",
    "apache-arrow": "^20.0.0",
    "arrow-cluster-layer": "workspace:*",
    "parquet-wasm": "^0.7.1"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20260305.0",
    "tsx": "^4.0.0",
    "vite": "^6.0.0",
    "vite-plugin-wasm": "^3.4.1",
    "typescript": "^5.0.0",
    "wrangler": "^4.0.0"
  }
}
