#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/jonathanstombaugh/Coding/arrow-supercluster/node_modules/.pnpm/tsdown@0.9.9_typescript@5.9.3/node_modules/tsdown/dist/node_modules:/Users/jonathanstombaugh/Coding/arrow-supercluster/node_modules/.pnpm/tsdown@0.9.9_typescript@5.9.3/node_modules/tsdown/node_modules:/Users/jonathanstombaugh/Coding/arrow-supercluster/node_modules/.pnpm/tsdown@0.9.9_typescript@5.9.3/node_modules:/Users/jonathanstombaugh/Coding/arrow-supercluster/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/jonathanstombaugh/Coding/arrow-supercluster/node_modules/.pnpm/tsdown@0.9.9_typescript@5.9.3/node_modules/tsdown/dist/node_modules:/Users/jonathanstombaugh/Coding/arrow-supercluster/node_modules/.pnpm/tsdown@0.9.9_typescript@5.9.3/node_modules/tsdown/node_modules:/Users/jonathanstombaugh/Coding/arrow-supercluster/node_modules/.pnpm/tsdown@0.9.9_typescript@5.9.3/node_modules:/Users/jonathanstombaugh/Coding/arrow-supercluster/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../dist/run.js" "$@"
else
  exec node  "$basedir/../../dist/run.js" "$@"
fi
